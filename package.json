{
    "name": "node.js",
    "version": "1.0.0",
    "description": "A Node.js Express project with Babel and Webpack in TypeScript",
    "main": "dist/index.js",
    "scripts": {
        "dev": "cross-env NODE_ENV=development webpack --config webpack.dev.ts",
        "build": "cross-env NODE_ENV=production webpack --config webpack.prod.ts",
        "start": "npm run build && node dist/main.bundle.js",
        "test": "mocha --require @babel/register --require ts-node/register 'src/**/*.test.ts' 'test/**/*.test.ts'",
        "format": "prettier --config .prettierrc --write \"./**/*.{js,ts,json}\""
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@babel/cli": "^7.19.3",
        "@babel/core": "^7.20.5",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-typescript": "^7.18.6",
        "@babel/register": "^7.18.9",
        "@types/body-parser": "^1.19.2",
        "@types/chai": "^4.3.4",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.15",
        "@types/mocha": "^10.0.1",
        "@types/node": "^18.11.15",
        "@types/webpack-node-externals": "^2.5.3",
        "@typescript-eslint/eslint-plugin": "^5.46.1",
        "@typescript-eslint/parser": "^5.46.1",
        "babel-loader": "^9.1.0",
        "chai": "^4.3.7",
        "chai-http": "^4.3.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.30.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "mocha": "^10.2.0",
        "nodemon": "^2.0.20",
        "pkg": "^5.8.0",
        "prettier": "^2.8.1",
        "rimraf": "^3.0.2",
        "terser-webpack-plugin": "^5.3.6",
        "ts-node": "^10.9.1",
        "tsconfig-paths-webpack-plugin": "^4.0.0",
        "typescript": "^4.9.4",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.1",
        "webpack-merge": "^5.8.0",
        "webpack-node-externals": "^3.0.0",
        "webpack-shell-plugin-next": "^2.3.1"
